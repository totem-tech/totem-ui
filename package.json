		"@babel/core": "^7.21.3",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"babel-loader": "^9.1.2",
		"compression-webpack-plugin": "^10.0.0",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.7.3",
		"dotenv-webpack": "^8.0.1",
		"eslint": "^8.42.0",
		"express": "^4.17.1",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.5.0",
		"ignore-loader": "^0.1.2",
		"nodemon": "^2.0.2",
		"npm-run-all": "^4.1.5",
		"style-loader": "^3.3.2",
		"url-loader": "^4.1.1",
